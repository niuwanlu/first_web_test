<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sprhib.model.Users" table="USERS" catalog="Sprhib">
        <id name="id" type="int" column="id" length="3">
            <generator class="identity"/>
        </id>
        <property name="username" column="username" length="10" not-null="true" type="java.lang.String"/>
        <property name="password" column="password" length="20" not-null="true" type="java.lang.String"/>
    </class>
</hibernate-mapping>